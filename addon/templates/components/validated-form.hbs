{{yield (hash
  model=model
  input=(component 'validated-input'
    model=model
    submitted=submitted
    validateBeforeSubmit=validateBeforeSubmit
    config=config
  )
  submit=(component 'validated-button'
    class   = submitClass
    type    = "submit"
    loading = submitTask.isRunning
    label   = _submitLabel
    config  = config
  )
)}}
