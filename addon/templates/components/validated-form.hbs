{{yield (hash
  model=model
  loading=loading
  input=(component 'validated-input'
    model=model
    submitted=submitted
    validateBeforeSubmit=validateBeforeSubmit
    config=config
  )
  submit=(component 'validated-button'
    class   = submitClass
    type    = "submit"
    loading = loading
    label   = _submitLabel
    config  = config
  )
)}}
